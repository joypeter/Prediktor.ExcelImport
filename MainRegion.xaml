<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            xmlns:Views="clr-namespace:Prediktor.Carbon.Configuration.Views;assembly=Prediktor.Carbon.Configuration" 
              x:Class="Prediktor.ExcelImport.MainRegion" 
           xmlns:local="clr-namespace:Prediktor.ExcelImport" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Views:TabbedView x:Name="TabbedView"  Grid.Row="0" DataContext="{Binding TabbedViewModel}"/>
    </Grid>
</UserControl>
